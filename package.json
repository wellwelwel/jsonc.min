{
  "name": "jsonc.min",
  "version": "1.0.0",
  "description": "✨ Faster and safer JSON and JSONC minify, parse and stringify for JavaScript (Browser compatible) — 9kB.",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellwelwel/jsonc.min.git"
  },
  "bugs": {
    "url": "https://github.com/wellwelwel/jsonc.min/issues"
  },
  "author": "https://github.com/wellwelwel",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/wellwelwel"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0",
    "bun": ">=1.0.0",
    "deno": ">=1.30.0"
  },
  "scripts": {
    "build": "tsc",
    "test:node": "poku --platform=node --parallel",
    "test:bun": "poku --platform=bun --parallel",
    "test:deno": "poku --platform=deno --parallel",
    "lint": "npx @biomejs/biome lint && prettier --check .",
    "lint:fix": "npx @biomejs/biome lint --write && prettier --write .github/workflows/*.yml .",
    "update": "pu minor && npm i && npm audit fix",
    "postupdate": "npm run lint:fix"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "packages-update": "^2.0.0",
    "poku": "^2.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "keywords": [
    "json",
    "jsonc",
    "minify",
    "minifier",
    "parse",
    "parser",
    "stringify",
    "jsonc-to-json"
  ]
}
